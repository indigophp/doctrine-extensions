{
    "name": "indigophp/doctrine-extensions",
    "description": "Custom Doctrine extensions",
    "license": "MIT",
    "keywords": ["doctrine", "extensions"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "doctrine/orm": "~2.4",
        "gedmo/doctrine-extensions": "~2.3",
        "phpspec/phpspec": "~2.1",
        "henrikbjorn/phpspec-code-coverage" : "~1.0"
    },
    "suggest": {
        "doctrine/orm": "Doctrine 2 ORM",
        "gedmo/doctrine-extensions": "Doctrine Behavioral Extensions"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Doctrine\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
